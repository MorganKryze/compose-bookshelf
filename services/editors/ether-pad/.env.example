# Ether pad Environment Variables
# Please copy this file to .env and set the appropriate values
# execute: 'cp .env.example .env'

# Default docker settings
# Id and group for file permissions
PUID=1000
PGID=1000
# Timezone setting
TZ=Europe/London
# Docker volumes path
DOCKER_VOLUMES_PATH=/mnt/pool/docker/volumes

# Database settings
DOCKER_COMPOSE_APP_ADMIN_PASSWORD=your_secure_password_here
DOCKER_COMPOSE_POSTGRES_PASSWORD=your_second_secure_password_here
DOCKER_COMPOSE_APP_DB_CHARSET=utf8mb4
DOCKER_COMPOSE_POSTGRES_DATABASE=etherpad
DOCKER_COMPOSE_POSTGRES_PORT=5432
DOCKER_COMPOSE_POSTGRES_USER=admin
DOCKER_COMPOSE_APP_DISABLE_IP_LOGGING=false