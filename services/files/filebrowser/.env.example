# execute: 'cp .env.example .env'

# Id and group for file permissions
PUID=1000
PGID=1000
# Timezone setting
TZ=Europe/London

# Paths
DOCKER_VOLUMES_PATH=/mnt/pool/docker-volumes
FILEBROWSER_CONFIG=data/config.yaml
BASE_FOLDER=/mnt/pool/data